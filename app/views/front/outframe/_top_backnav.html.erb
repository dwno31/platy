<div class="backbutton">
  <span onclick="back_platy();" class="glyphicon glyphicon-menu-left"></span>
</div>
<div class="outlink-shopname">
  <span><%=@record.title%></span>
</div>

<script>
  var origin_length = window.history.length;
  function back_platy(){
  var back_length = origin_length - window.history.length - 1;
  window.history.go(back_length);

  }

</script>